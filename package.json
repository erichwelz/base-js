{
  "name": "setup-unit-test",
  "version": "1.0.0",
  "description": "this project demonstrates how to write unit test in javascript by using jest framework ",
  "engines": {
    "node": ">=12"
  },
  "devDependencies": {
    "jest": "^25.1.0",
    "standard": "^16.0.1"
  },
  "main": "index.js",
  "scripts": {
    "unit": "jest",
    "unit:watch": "jest --watchAll",
    "unit:watch:noCoverage": "jest --watchAll --collectCoverage=false"
  },
  "keywords": [
    "javascript",
    "jest"
  ],
  "license": "ISC",
  "standard": {
    "env": [
      "jest"
    ]
  },
  "dependencies": {
    "ramda": "^0.27.1"
  }
}
